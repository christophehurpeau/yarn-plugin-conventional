{
  "cacheDirectory": "./node_modules/.cache/jest",
  "testMatch": [
    "<rootDir>/sources/**/__tests__/**/*.?(m)js",
    "<rootDir>/sources/**/*.test.?(m)js"
  ],
  "collectCoverageFrom": ["sources/**/*.?(m)js"],
  "moduleFileExtensions": ["js", "json"],
  "extensionsToTreatAsEsm": [],
  "transform": {
    "^.+\\.ts$": [
      "jest-esbuild",
      {
        "format": "esm"
      }
    ]
  }
}
